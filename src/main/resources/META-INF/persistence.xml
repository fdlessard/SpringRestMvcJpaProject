<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence" version="2.1">

    <persistence-unit name="RestSpringMvcJpaAppPersitenceUnit">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <properties>
            <property name="eclipselink.target-database" value="PostgreSQL"/>
            <property name="eclipselink.logging.level" value="INFO"/>
            <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/SampleProject"/>
            <property name="javax.persistence.jdbc.user" value="sampleuser"/>
            <property name="javax.persistence.jdbc.password" value="8F54DF4B294668E6139D406E12FE7CCA"/>

        </properties>
    </persistence-unit>

</persistence>
